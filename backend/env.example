# Meraki Admin JIT - Backend environment template
# Copy to .env and fill in values. Do not commit .env.
#
# Generate SECRET_KEY: openssl rand -hex 32
# setup.sh will replace the placeholder below with a random key when creating .env

# Required: session and JWT signing (replace placeholder before first run)
SECRET_KEY=your-super-secret-key-change-this-in-production

# Required: app and Duo SAML
APP_URL=
FRONTEND_URL=http://localhost:3000
ALLOWED_ORIGINS=http://localhost:3000

# Duo SAML (from Duo Admin Panel)
DUO_ENTITY_ID=
DUO_SSO_URL=
DUO_SLO_URL=
DUO_X509_CERT=

# Optional: Meraki Dashboard API (or use 1Password via .env.op)
MERAKI_DASHBOARD_API_KEY=

# Optional: Flask
FLASK_ENV=development
FLASK_HOST=0.0.0.0
FLASK_PORT=5001

# Optional: session
SESSION_TYPE=filesystem
SESSION_COOKIE_HTTPONLY=true
SESSION_COOKIE_SAMESITE=Lax
SESSION_COOKIE_SECURE=false
PERMANENT_SESSION_LIFETIME=43200
SESSION_REDIS=redis://localhost:6379

# Optional: JWT and logging
JWT_EXPIRY_SECONDS=43200
LOG_LEVEL=INFO
WERKZEUG_LOG_LEVEL=WARNING
